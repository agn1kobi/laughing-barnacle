{
  "name": "laughing-barnacle",
  "version": "0.0.0",
  "description": "Movie review API",
  "author": "Ramanuj Baruah",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "tsc --build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "npx jest --config jest.config.ts",
    "dev": "npx tsx src/index.ts",
    "start": "node dist/src/index.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-basic-auth": "^1.2.1",
    "ioredis": "^5.8.0",
    "jsonwebtoken": "^9.0.2",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "reflect-metadata": "0.2.2",
    "rxjs": "^7.8.2",
    "ts-pattern": "^5.8.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.27",
    "typescript": "^5.9.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@swc/jest": "^0.2.39",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.6.2",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{js,jsx,json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
